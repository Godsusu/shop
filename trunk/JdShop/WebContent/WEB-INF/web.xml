<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JdShop</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>UserController</display-name>
    <servlet-name>UserController</servlet-name>
    <servlet-class>cn.edu.guet.web.servlet.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/404Error.jsp</location>
  </error-page>
  <servlet>
    <description></description>
    <display-name>RoleController</display-name>
    <servlet-name>RoleController</servlet-name>
    <servlet-class>cn.edu.guet.web.servlet.RoleController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoleController</servlet-name>
    <url-pattern>/role</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PermissionController</display-name>
    <servlet-name>PermissionController</servlet-name>
    <servlet-class>cn.edu.guet.web.servlet.PermissionController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PermissionController</servlet-name>
    <url-pattern>/permission</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CustomerController</display-name>
    <servlet-name>CustomerController</servlet-name>
    <servlet-class>cn.edu.guet.web.servlet.CustomerController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CustomerController</servlet-name>
    <url-pattern>/customer</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BaseServlet</display-name>
    <servlet-name>BaseServlet</servlet-name>
    <servlet-class>cn.edu.guet.web.servlet.base.BaseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BaseServlet</servlet-name>
    <url-pattern>/BaseServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>productController</display-name>
    <servlet-name>productController</servlet-name>
    <servlet-class>cn.edu.guet.web.servlet.productController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>productController</servlet-name>
    <url-pattern>/product</url-pattern>
  </servlet-mapping>
</web-app>